<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="a.talenting.com.talenting">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SigninActivity" />
        <activity android:name=".controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controller.setting.profile.ProfileActivity" />
        <activity android:name=".controller.setting.hosting.SetHostingListActivity" />
        <activity android:name=".controller.setting.hosting.SetHostingActivity" />
        <activity android:name=".controller.setting.profile.ProfileEditActivity" />
        <activity android:name=".controller.setting.hosting.SetHostingAddActivity" />
        <activity android:name=".controller.event.EventActivity" />
        <activity android:name=".controller.setting.event.SetEventListActivity"/>
        <activity android:name=".controller.setting.event.SetEventAddActivity"/>
        <activity android:name=".controller.setting.event.SetEventActivity"/>
    </application>

</manifest>